{
    "name": "EmbyServer",
    "id": "embyserver",
    "packaging_format": 1,
    "description": {
        "en": "EmbyServer package for YunoHost.",
        "fr": "EmbyServer pour YunoHost."
    },
    "version": "4.5.4.0~ynh1",
    "url": "https://emby.media/",
    "license": "non-free",
    "maintainer": {
        "name": "liberodark",
        "email": "liberodark@gmail.com"
    },
    "requirements": {
        "yunohost": ">= 4.1.7"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "mysql"
    ],
    "arguments": {
        "install": [{
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/emby",
                "default": "/emby"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
